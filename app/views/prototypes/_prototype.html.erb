<div class="card">
  <%= link_to image_tag(prototype.image, class: :card__img ),user_path(prototype.id) %>
  <div class="card__body">
    <%= link_to prototype.title, user_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to prototype.user.name, user_path(prototype.id), class: :card__user %>
  </div>
</div>